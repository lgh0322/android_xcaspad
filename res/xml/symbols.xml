<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="120" android:keyLabel="x" android:keyEdgeFlags="left"/>
        <Key android:codes="121" android:keyLabel="y"/>
        <Key android:codes="122" android:keyLabel="z"/>
        <Key android:codes="55" android:keyIcon="@drawable/siete" />
        <Key android:codes="56" android:keyIcon="@drawable/ocho"/>
        <Key android:codes="57" android:keyIcon="@drawable/nueve"/>
        <!--<Key android:codes="61" android:keyLabel="="/>-->
        
        <Key android:keyOutputText="sqrt(" android:keyLabel="âˆš"/>
        <Key android:codes="94" android:keyLabel="^" android:keyEdgeFlags="right"/>
        
      
        
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
                android:keyWidth="20%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
    
    <Row>
        <Key android:codes="65" android:keyLabel="A" android:keyEdgeFlags="left"/>
        <Key android:codes="66" android:keyLabel="B"/>
        <Key android:codes="67" android:keyLabel="C"/>
        <Key android:codes="52" android:keyIcon="@drawable/cuatro"/>
        <Key android:codes="53" android:keyIcon="@drawable/cinco"/>
        <Key android:codes="54" android:keyIcon="@drawable/seis"/>
        <Key android:codes="42" android:keyLabel="*"/>
        <Key android:codes="47" android:keyLabel="/"/>
        
        <Key android:codes="40" android:keyLabel="("/>
        <Key android:codes="41" android:keyLabel=")"/>
        
    </Row>
    
    <Row>
        <Key android:codes="58" android:keyLabel=":"/>
        <Key android:codes="61" android:keyLabel="="/>
      
        <Key android:codes="48" android:keyIcon="@drawable/cero" />
        <Key android:codes="49" android:keyIcon="@drawable/uno"/>
        <Key android:codes="50" android:keyIcon="@drawable/dos"/>
        <Key android:codes="51" android:keyIcon="@drawable/tres"/>
        <Key android:codes="43" android:keyLabel="+" />
        <Key android:codes="45" android:keyLabel="-"/>
        
        <Key android:codes="91" android:keyLabel="["/>
        <Key android:codes="93" android:keyLabel="]" android:keyEdgeFlags="right"/>
        
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <!--<Key android:keyOutputText="ans()" android:keyLabel="ans"
                android:keyWidth="20%p" android:keyEdgeFlags="left" />-->
         <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift"
                android:keyWidth="15%p" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="left"/>       
        <Key android:codes="-2" android:keyLabel="ABC" android:keyWidth="15%p" />
        <!-- <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="30%p" 
                android:isRepeatable="true"/>-->
        <Key android:codes="-100" android:keyIcon="@drawable/gear2"/>
        <Key android:codes="46" android:keyLabel="."/>
        <Key android:codes="44" android:keyLabel=","/>
        <Key android:codes="59" android:keyLabel=";"/>
        
        <!--<Key android:keyOutputText="f(" android:keyLabel="f"/>-->
        <Key android:codes="123,125" android:keyLabel="{}"/>
        
         <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
                android:keyWidth="20%p" android:keyEdgeFlags="right" />
        
    </Row>
</Keyboard>
